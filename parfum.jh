

entity Client {
  nom 				String
  prenom			String
  tel				String
}

entity Categorie {
	code			String
    description		String
}

entity Produit {
	code			String
    description		String
    prix			BigDecimal
    dateCreation	LocalDate
}

relationship ManyToOne {
Produit to Categorie
}

dto all with mapstruct
service * with serviceClass
paginate all with pagination
filter *


